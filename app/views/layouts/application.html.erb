  <section id="section-title">
    <h1><a href="home">TYPECORE</a><span style="rgb(46,46,46); margin-left: -1px">.</span></h1>
    <h4>A Rock solid theme.</h4>
    <ul id="top-social-list">
      <li><i class="fa fa-home"></i></li>
      <li><i class="fa fa-twitter"></i></li>
      <li><i class="fa fa-facebook"></i></li>
      <li><i class="fa fa-google"></i></li>
      <li><i class="fa fa-rss"></i></li>
      <li><i class="fa fa-share-alt"></i></li>
    </ul>
  </section>
  <section id="page-content">
    <div id="left-social">
      <ul id="left-social-list">
        <li><i class="fa fa-home"></i></li>
        <li><i class="fa fa-twitter"></i></li>
        <li><i class="fa fa-facebook"></i></li>
        <li><i class="fa fa-google"></i></li>
        <li><i class="fa fa-rss"></i></li>
        <li><i class="fa fa-share-alt"></i></li>
      </ul>
    </div>
    <% if !current_page?(console_path) %>
      <div id="left-recent">
        <div id="left-recent-top">
          <h3>Recent</h3></div>
        <div id="left-recent-tab">
          <ul>
            <li class="active"><i class="fa fa-clock-o clock-button" onclick="updateDisplay('clock')"></i></li>
            <li><i class="fa fa-star star-button" onclick="updateDisplay('star')"></i></li>
            <li><i class="fa fa-comment comment-button" onclick="updateDisplay('comment')"></i></li>
          </ul>
        </div>
        <script>
          function updateDisplay(elem) {
              $('.clock').hide();
              $('.star').hide();
              $('.comment').hide();
              $('.'+elem).show();
            }
        </script>
        <div id="left-recent-tab-display">
          <ul class="clock">
            <% @i = -1 %>
            <% @num = num * -1 %>
            
            <% while @i >= @num  do %>
              <% if (!@current_art[@i].nil?) %>
                <li><a href="article?query=<%= @current_art[@i].id  %>">
                  <div>
                    <img src="<%= @current_art[@i].picture %>">
                    <h4 ><span style="color: rgb(230, 67, 56);">FEATURED</span> / <span style="color: rgb(230, 67, 56);text-transform: uppercase"><%= @current_art[@i].category %>D</span></h4>
                    <h5 class="current-art-title-<%= @i %>"><%= @current_art[@i].title %></h5>
                    <br class="clear-float" />
                    <h3 style="padding-bottom: 10px;"><%= @current_art[@i].created_at.to_s(format = :long) %></h3>
                    </div>
                    </a>
                  </li style="margin-bottom: 20px;">
              <br class="clear-float" />
              <% end %>
              <% @i -=1 %>
            <% end %>
          </ul>
          <ul class="star">
            <% @i = 0 %>
            
            <% while @i < @num  do %>
            <% @arts_hot = Article.where("id=?", @a[@i][0]) %>
              <% if (@arts_hot[0].exists?) %>
                <li><a href="article?query=<%= @arts_hot[0].id  %>">
                  <div>
                    <img src="<%= @arts_hot[0].picture %>">
                    <h4 ><span style="color: rgb(230, 67, 56);">FEATURED</span> / <span style="color: rgb(230, 67, 56);text-transform: uppercase"><%= @arts_hot[0].category %>D</span></h4>
                    <h5 class="current-art-title-<%= @i %>"><%= @arts_hot[0].title %></h5>
                    <br class="clear-float" />
                    <h3 style="padding-bottom: 10px;"><%= @arts_hot[0].created_at.to_s(format = :long) %></h3>
                    </div>
                    </a>
                  </li style="margin-bottom: 20px;">
                <br class="clear-float" />
              <% end %>
              <% @i +=1 %>
            <% end %>
          </ul>
          <ul class="comment">
            <li>
              <a href="article.html">
                <div><img src="http://2.gravatar.com/avatar/898c52f8de33246d15b4264d2e2ef7cf?s=192&d=mm&r=g">
                  <h4><span style="color: lightslategray; text-transform: uppercase; font-size: 12px;">Alexander Says</span></h4>
                  <h5 style="font-weight: 300;">Road Bike Party 2 with Martyn Ashton</h5>
                </div>
              </a>
            </li>
            <br class="clear" />
            <li>
              <a href="article.html">
                <div><img src="http://demo.alxmedia.se/typecore/wp-content/uploads/sites/9/2013/10/2e9b0cb2-320x320.jpg">
                  <h4><span style="color: lightslategray; text-transform: uppercase; font-size: 12px;">Alexander Says</span></h4>
                  <h5 style="font-weight: 300;">You really went all crazy out there</h5>
